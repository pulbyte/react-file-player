{
  "name": "react-file-player",
  "version": "1.0.1",
  "description": "A React component for playing media from url of video",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "start": "rollup -c -w",
    "clean": "rimraf dist",
    "build": "rollup -c",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --fix",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pulbyte/react-file-player.git"
  },
  "author": "Pul Byte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pulbyte/react-file-player/issues"
  },
  "homepage": "https://github.com/pulbyte/react-file-player",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "babel-preset-react-app": "^10.0.0",
